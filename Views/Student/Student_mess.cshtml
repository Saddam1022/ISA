
@{
    ViewData["Title"] = "Student_mess";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="~/data/css/stylesheet.css">
    <script src="https://kit.fontawesome.com/67ba5841f4.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <title>HMS</title>
</head>

<body>
    <div class="bgm">
        <header>
            <nav class="navbar navbar-expand-sm navbar-dark">
                <a href="#" class="navbar-brand">StarHostel</a>
                <button class="navbar-toggler" data-toggle="collapse" data-target="#Menu">
                    <span class="navbar-toggler icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="Menu">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link " href="/Home/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/ContactUs">ContactUs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Login">Log out</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <div id='main-content' class="card-deck" style="margin: 50px 0 0 0">

            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Monday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="mlunch" value="@ViewData["mlunch"]" >
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="mdinner" value="@ViewData["mdinner"]" >
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Tuesday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly  name="tlunch" value="@ViewData["tlunch"]" >
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="tdinner" value="@ViewData["tdinner"]" >
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Wednesday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="wlunch" value="@ViewData["wlunch"]" >
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="wdinner" value="@ViewData["wdinner"]" >
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Thursday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="thlunch" value="@ViewData["thlunch"]" >
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="thdinner" value="@ViewData["thdinner"]" >
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Friday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="flunch" value="@ViewData["flunch"]" >
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="fdinner" value="@ViewData["flunch"]">
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Saturday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="slunch" value="@ViewData["slunch"]">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="sdinner" value="@ViewData["sdinner"]">
                    </div>

                </div>
            </div>
            <div class="card" style="width: 10rem;">
                <div class="card-body">
                    <h5 class="card-title"><b>Sunday</b></h5>
                    <div class="form-group">

                        <input type="text" class="form-control" readonly name="sulunch" value="@ViewData["sulunch"]">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" readonly name="sudinner" value="@ViewData["sudinner"]">
                    </div>

                </div>
            </div>
        </div>
        <br>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4 col-sm-4 col-xs-12"></div>
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <button class="btn btn-primary btn-lg float-left" type="submit"> <a class="nav-link " href="/Student/Studentportal">Back</button></a>


                </div>
            </div>

        </div>



        <footer class="border-top footer text-muted">
            <div class="container1 text-center py-3">
                &copy; 2020 - webApp - <a asp-area="">Privacy</a>
            </div>
        </footer>
    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
</body>

</html>